{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "pipes": {
      "type": "library",
      "root": "libs/pipes",
      "entryFile": "index",
      "sourceRoot": "libs/pipes/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pipes/tsconfig.lib.json"
      }
    },
    "filters": {
      "type": "library",
      "root": "libs/filters",
      "entryFile": "index",
      "sourceRoot": "libs/filters/src",
      "compilerOptions": {
        "tsConfigPath": "libs/filters/tsconfig.lib.json"
      }
    }
  }
}